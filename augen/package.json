{
  "name": "@sjcrh/augen",
  "version": "1.0.0",
  "description": "Automatic generator of server routes, unit tests, and API documentation",
  "type": "module",
  "main": "src/augen.js",
  "bin": "cli.j",
  "scripts": {
    "predev": "ts-node-esm test/prep.js",
    "dev": "ts-node-esm test/app.js",
    "pretest": "ts-node-esm test/prep.js && typia generate --input test/checkers/raw --output test/checkers/transformed",
    "test": "ts-node-esm test/unit.spec.js",
    "doc": "typedoc --json ../public/docs.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "typedoc": "^0.24.8",
    "typedoc-plugin-replace-text": "^3.1.0",
    "typia": "^4.1.14"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "tape": "^5.2.2"
  }
}
