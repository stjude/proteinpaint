{
	"presets": [
		"@babel/preset-typescript",
		[
			"@babel/preset-env",
			{
				"targets": ">1%, last 2 years",
				"bugfixes": true
			}
		]
	],

	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-transform-runtime",
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread"
	]
}