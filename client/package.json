{
  "name": "@sjcrh/proteinpaint-client",
  "version": "2.74.2",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "dist/app.js",
  "module": "dist/app.js",
  "browser": "dist/app.js",
  "imports": {
    "#common/*": "./common/*.js",
    "#dom/*": "./dom/*.js",
    "#filter": "./filter/filter.js",
    "#filter/*": "./filter/*.js",
    "#mass/*": "./mass/*.js",
    "#plots/*": "./plots/*",
    "#plots/disco/*": "./plots/disco/*",
    "#rx": "./rx/index.js",
    "#shared/*": "@sjcrh/proteinpaint-server/shared/*",
    "#src/*": "./src/*.js",
    "#termdb/*": "./termdb/*.js",
    "#termsetting": "./termsetting/termsetting.ts",
    "#termsetting/handlers/*": "./termsetting/handlers/*.ts",
    "#tw": "./tw/*",
    "#types": "./shared/types/index.ts"
  },
  "scripts": {
    "predev0": "node ./test/import-specs.js name=? && rm -f ../public/bin/*proteinpaint.js",
    "dev0": "rm -rf ../public/bin; webpack --env NODE_ENV=development --watch --progress --color",
    "dev": "./dev.sh",
    "pretest": "./test/pretest.js",
    "test": "./test.sh *.spec.js",
    "test:unit": "./test.sh *.unit.spec.*",
    "test:integration": "./test.sh *.integration.spec.*",
    "tsc": "tsc -p tsconfig-x.json",
    "examples": "node ./test/import-specs.js name=_x_.examples && npm run tape",
    "//TODO": "# replace rollup with esbuild",
    "rollw": "rm -rf dist && rollup -c ./rollup.config.js -w --bundleConfigAsCjs",
    "build": "rm -rf dist && rollup -c ./rollup.config.js",
    "prepack": "rm -rf dist && rollup -c ./rollup.config.js && npm run sedver",
    "sedver": "VER=$(node -p \"require('./package.json').version\") && sed -i.bk \"s|___current-proteinpaint-client-version___|$VER|\" dist/app*.js && rm dist/*.js.bk"
  },
  "author": "",
  "license": "SEE LICENSE IN ./LICENSE",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.21.4",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-dynamic-import-vars": "^1.4.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/d3": "^7.4.3",
    "@types/openseadragon": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.7.1",
    "d3": "^7.6.1",
    "d3-beeswarm": "0.0.4",
    "d3-regression": "^1.3.10",
    "debounce": "^1.2.0",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "glob": "^10.4.1",
    "highlight.js": "~11.2.0",
    "html-webpack-plugin": "^5.5.0",
    "minimatch": "^3.1.2",
    "node-notifier": "^9.0.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "normalize.css": "^8.0.1",
    "ol": "^10.0.0",
    "openseadragon": "^4.1.1",
    "partjson": "^0.58.2",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "read-cache": "^1.0.0",
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^4.0.2",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "tape": "^5.2.2",
    "tape-run": "^11.0.0",
    "three": "^0.152.2",
    "typedoc": "^0.24.8",
    "webpack": "^5.90.3",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-notifier": "^1.15.0"
  },
  "peerDependencies": {
    "postcss": "^8.4.41",
    "postcss-import": "^14.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/proteinpaint",
    "directory": "client"
  },
  "files": [
    "dist/*",
    "src/databrowser/dictionary.parse.js",
    "src/spliceevent.prep.js",
    "src/spliceevent.a53ss.js",
    "src/spliceevent.exonskip.js",
    "LICENSE/*"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  }
}
