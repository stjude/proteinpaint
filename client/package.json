{
  "name": "@sjcrh/proteinpaint-client",
  "version": "2.26.1",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "dist/app.js",
  "module": "dist/app.js",
  "imports": {
    "#src/*": "./src/*.js",
    "#common/*": "./common/*.js",
    "#dom/*": "./dom/*.js",
    "#filter": "./filter/filter.js",
    "#filter/*": "./filter/*.js",
    "#mass/*": "./mass/*.js",
    "#plots/*": "./plots/*",
    "#plotsts/*": "./plots/*.js",
    "#plots/disco/*": "./plots/disco/*",
    "#rx": "./rx/index.js",
    "#shared/*": "@sjcrh/proteinpaint-server/shared/*",
    "#shared/types/*": "@sjcrh/proteinpaint-server/shared/types/*",
    "#termdb/*": "./termdb/*.js",
    "#termsetting": "./termsetting/termsetting.ts",
    "#termsetting/handlers/*": "./termsetting/handlers/*.ts"
  },
  "scripts": {
    "predev": "node ./test/import-specs.js name=? && rm -f ../public/bin/*proteinpaint.js",
    "dev": "webpack --env NODE_ENV=development --watch --progress --color",
    "pretest": "./test/pretest.js",
    "test": "node ./test/import-specs.js && npm run tape",
    "pretest:unit": "webpack --config=./webpack.test.config.js --env TEST_TYPE=unit",
    "test:unit": "cat ../public/bin/unittest.js | tape-run --static ../public",
    "tape": "cat ../public/bin/proteinpaint.js ./test/init.js | tape-run --static ../public",
    "pretest:integration": "webpack --config=./webpack.test.config.js --env TEST_TYPE=integration",
    "test:integration": "npm run tape:integration",
    "tape:integration": "cat ../public/bin/testrunproteinpaint.js ./test/initIntegration.js | tape-run --static ../public",
    "examples": "node ./test/import-specs.js name=_x_.examples && npm run tape",
    "rollw": "rm -rf dist && rollup -c ./rollup.config.js -w --bundleConfigAsCjs",
    "build": "rm -rf dist && rollup -c ./rollup.config.js",
    "prepack": "rm -rf dist && rollup -c ./rollup.config.js"
  },
  "author": "",
  "license": "SEE LICENSE IN ./LICENSE",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.21.4",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-dynamic-import-vars": "^1.4.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.7.1",
    "d3": "^7.6.1",
    "d3-beeswarm": "0.0.4",
    "d3-regression": "^1.3.10",
    "debounce": "^1.2.0",
    "highlight.js": "~11.2.0",
    "html-webpack-plugin": "^5.5.0",
    "minimatch": "^3.1.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "normalize.css": "^8.0.1",
    "partjson": "^0.58.1",
    "postcss-import": "^14.1.0",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "read-cache": "^1.0.0",
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^4.0.2",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "tape": "^5.2.2",
    "tape-run": "^10.0.0",
    "three": "^0.152.2",
    "typedoc": "^0.24.8",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-notifier": "^1.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/proteinpaint",
    "directory": "client"
  },
  "files": [
    "dist/*",
    "src/databrowser/dictionary.parse.js",
    "src/spliceevent.prep.js",
    "src/spliceevent.a53ss.js",
    "src/spliceevent.exonskip.js",
    "LICENSE/*"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  }
}
