{
  "name": "@stjude/proteinpaint-client",
  "version": "2.4.0",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "dist/app.js",
  "module": "dist/app.js",
  "imports": {
    "#src/*": "./src/*.js",
    "#common/*": "./common/*.js",
    "#dom/*": "./dom/*.js",
    "#filter": "./filter/filter.js",
    "#filter/*": "./filter/*.js",
    "#mass/*": "./mass/*.js",
    "#plots/*": "./plots/*.js",
    "#rx": "./rx/index.js",
    "#shared/*": "./shared/*.js",
    "#termdb/*": "./termdb/*.js",
    "#termsetting": "./termsetting/termsetting.js",
    "#termsetting/handlers/*": "./termsetting/handlers/*.js"
  },
  "scripts": {
    "predev": "node ./test/import-specs.js name=? && rm -f ../public/bin/*proteinpaint.js",
    "dev": "webpack --env NODE_ENV=development --watch --progress --color",
    "pretest": "./test/pretest.js",
    "test": "node ./test/import-specs.js && npm run tape",
    "tape": "cat ../public/bin/proteinpaint.js ./test/init.js | tape-run --static ../public",
    "examples": "node ./test/import-specs.js name=_x_.examples && npm run tape",
    "rollw": "rm -rf dist && rollup -c ./rollup.config.js -w",
    "build": "rm -rf dist && rollup -c ./rollup.config.js && npm pack"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-dynamic-import-vars": "^1.4.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.7.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "postcss-import": "^14.1.0",
    "prettier": "1.18.2",
    "read-cache": "^1.0.0",
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "^3.3.1",
    "tape": "^5.2.2",
    "tape-run": "^10.0.0",
    "webpack": "^5.7.3",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-notifier": "^1.15.0"
  },
  "dependencies": {
    "d3": "^7.6.1",
    "d3-beeswarm": "0.0.4",
    "debounce": "^1.2.0",
    "highlight.js": "~11.2.0",
    "normalize.css": "^8.0.1",
    "partjson": "^0.58.1",
    "process": "^0.11.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/pp-dist"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist/*"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  }
}
