{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
  },
  "types": ["node"],
  "include": ["./**/*.ts"],
  "exclude": ["./**/*.type.spec.ts", "./package/*"],
  "paths": {
    "#shared/*": ["./shared/*"],
    "#shared/types/*": ["./shared/types/*"],
