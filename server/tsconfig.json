{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "#src/*": [
        "./src/*"
      ],
      "#shared/*": [
        "./shared/*"
      ]
    },
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules"
  ]
}