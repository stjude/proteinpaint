{
  "name": "@sjcrh/proteinpaint-server",
  "version": "2.86.0",
  "type": "module",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "src/app.js",
  "module": "src/app.js",
  "bin": "start.js",
  "imports": {
    "#types": "@sjcrh/proteinpaint-types",
    "#types/*": "@sjcrh/proteinpaint-types/*",
    "#shared/*": "@sjcrh/proteinpaint-shared/*",
    "#src/*": "./src/*",
    "#routes/*": "./routes/*"
  },
  "scripts": {
    "dev": "npm run start",
    "prestart": "tsx emitImports.js dev > server.js",
    "start": "tsx watch . /start.js",
    "test:unit": "tsx emitImports.js unit > serverTests.js && tsx serverTests.js && rm -rf ./cache",
    "getconf": "../build/getConfigProp.js",
    "doc": "../augen/build.sh routes shared/types/routes shared/checkers ../public/docs/server",
    "mjs": "esbuild \"$DIR/*.ts\" --platform=node --outdir=\"$DIR\" --format=esm",
    "cjs": "esbuild \"$DIR/*.ts\" --platform=node --outdir=\"$DIR\" --format=cjs",
    "build": "./build.sh",
    "prepack": "npm run build",
    "postpack": "./dedupjs.sh",
    "dedup": "./dedupjs.sh",
    "//todo": "refactor or deprecate the scripts below",
    "pretest": "tsc && ./test/pretest.js",
    "pretest:type": "npm run checkers",
    "pretest:integration": "tsc",
    "test:integration": "echo 'TODO: server integration tests'",
    "test:tsc": "tsc --esModuleInterop --noEmit --allowImportingTsExtensions ./shared/types/test/*.type.spec.ts"
  },
  "author": "",
  "license": "SEE LICENSE IN ./LICENSE",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/register": "^7.14.5",
    "@types/node": "^20.11.24",
    "@types/tough-cookie": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "babel-loader": "^8.2.2",
    "esbuild": "^0.19.12",
    "glob": "^10.4.5",
    "node-watch": "^0.7.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.8.8",
    "tape": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsx": "^4.7.1",
    "typedoc": "^0.26.11",
    "typedoc-plugin-missing-exports": "^3.0.0",
    "typedoc-plugin-replace-text": "^4.0.0",
    "typescript": "^5.6.3",
    "typia": "^4.1.14"
  },
  "dependencies": {
    "@sjcrh/augen": "2.86.0",
    "@sjcrh/proteinpaint-rust": "2.84.0",
    "@sjcrh/proteinpaint-shared": "2.86.0",
    "@sjcrh/proteinpaint-types": "2.86.0",
    "better-sqlite3": "^9.4.1",
    "body-parser": "^1.15.2",
    "canvas": "~2.11.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.5",
    "d3": "^7.6.1",
    "deep-object-diff": "^1.1.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.1.5",
    "got": "^14.2.0",
    "image-size": "^0.5.5",
    "jsonwebtoken": "^9.0.0",
    "jstat": "^1.9.3",
    "ky": "^1.2.1",
    "lazy": "^1.0.11",
    "micromatch": "^4.0.5",
    "minimatch": "^3.1.2",
    "node-fetch": "^2.6.1",
    "partjson": "^0.58.2",
    "tiny-async-pool": "^1.2.0",
    "tough-cookie": "^4.1.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/proteinpaint",
    "directory": "server"
  },
  "files": [
    "bin.js",
    "dataset/*.js",
    "genome/*.js",
    "routes/*.js",
    "start.js",
    "src/app.js",
    "src/serverconfig.js",
    "src/run_R.js",
    "src/mds3.gdc.filter.js",
    "src/checkReadingFrame.js",
    "src/bedj.parseBed.js",
    "utils/*.R",
    "utils/*.py",
    "LICENSE/*"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  },
  "browserify": {
    "transform": [
      "browserify-css",
      [
        "babelify",
        {
          "babelrc": false,
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "@babel/plugin-syntax-dynamic-import",
            "@babel/plugin-transform-runtime"
          ]
        }
      ]
    ]
  }
}
