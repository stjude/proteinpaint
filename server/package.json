{
  "name": "@stjude/proteinpaint-server",
  "version": "2.9.4-0",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "server.js",
  "bin": "start.js",
  "imports": {
    "#shared/*": "./shared/*.js",
    "#src/*": "./src/*.js"
  },
  "scripts": {
    "dev": "webpack --env NODE_ENV=development --watch --color",
    "start": "nodemon server.js --watch ./server*.js* --watch genome --watch dataset",
    "pretest": "./test/pretest.js",
    "test": "webpack --env NODE_ENV=test exportsFilename=all-test-context.js && node test/serverTests.js",
    "test:unit": "webpack --env NODE_ENV=test exportsFilename=unit-test-context.js && node test/serverTests.js",
    "test:integration": "echo 'TODO: server integration tests'",
    "prepack": "webpack --env NODE_ENV=production",
    "response": "nodemon modules/test/test.server.js --watch src",
    "getconf": "../build/getConfigProp.js"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.14.5",
    "babel-loader": "^8.2.2",
    "node-watch": "^0.7.1",
    "nodemon": "^2.0.19",
    "prettier": "1.18.2",
    "tape": "^5.2.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-notifier": "^1.15.0"
  },
  "dependencies": {
    "@stjude/proteinpaint-rust": "2.7.1-0",
    "better-sqlite3": "^7.5.3",
    "body-parser": "^1.15.2",
    "canvas": "~2.9.3",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.5",
    "d3": "^7.6.1",
    "deep-object-diff": "^1.1.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.1.5",
    "got": "^11.5.1",
    "image-size": "^0.5.5",
    "jsonwebtoken": "^8.5.1",
    "jstat": "^1.9.3",
    "lazy": "^1.0.11",
    "node-fetch": "^2.6.1",
    "partjson": "^0.58.1",
    "tiny-async-pool": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/proteinpaint",
    "directory": "server"
  },
  "files": [
    "bin.js",
    "dataset/clinvar*.js",
    "dataset/gdc*.js",
    "genome",
    "src/serverconfig.js",
    "src/lines2R.js",
    "src/mds3.gdc.filter.js",
    "src/checkReadingFrame.js",
    "src/bedj.parseBed.js",
    "cards/index.json",
    "server.js*",
    "start.js",
    "shared/common.js",
    "shared/termdb.initbinconfig.js",
    "utils/*.R",
    "utils/rust/*.*",
    "utils/rust/src"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  },
  "browserify": {
    "transform": [
      "browserify-css",
      [
        "babelify",
        {
          "babelrc": false,
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "@babel/plugin-syntax-dynamic-import",
            "@babel/plugin-transform-runtime"
          ]
        }
      ]
    ]
  }
}
