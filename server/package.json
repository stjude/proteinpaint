{
  "name": "@sjcrh/proteinpaint-server",
  "version": "2.44.0",
  "type": "module",
  "description": "a genomics visualization tool for exploring a cohort's genotype and phenotype data",
  "main": "app/src.js",
  "bin": "server.js",
  "imports": {
    "#shared/*": "./shared/*",
    "#shared/types/*": "./shared/types/*",
    "#src/*": "./src/*",
    "#routes/*": "./routes/*"
  },
  "scripts": {
    "dev": "npm run start",
    "prestart": "tsx emitImports.js dev > server.js",
    "start": "tsx watch src/app.ts",
    "test:unit": "tsx emitImports.js unit > serverTests.js && tsx serverTests.js",
    "pretest:integration": "tsc --esModuleInterop genome/*.ts dataset/*.ts",
    "test:integration": "echo 'TODO: server integration tests'",
    "prepack": "npm run build",
    "postpack": "rm -f ./**/*.mjs",
    "getconf": "../build/getConfigProp.js",
    "doc": "../augen/build.sh routes shared/types/routes shared/checkers ../public/docs/server",
    "mjs": "esbuild \"$DIR/*.ts\" --platform=node --outdir=\"$DIR\" --out-extension:.js=.mjs",
    "prebuild": "DIR=genome npm run mjs; DIR=dataset npm run mjs; DIR=routes npm run mjs",
    "build": "npx esbuild start.js --bundle --platform=node --packages=external --format=esm --sourcemap > server.js",
    "postbuild": "./postbuild.sh",
    "//todo": "refactor or deprecate the scripts below",
    "pretest": "tsc --esModuleInterop genome/*.ts dataset/*.ts && ./test/pretest.js",
    "prepare": "ts-patch install",
    "precheckers": "tsc --esModuleInterop genome/*.ts dataset/*.ts",
    "checkers": "webpack --config=./test/webpack.config.js && node test/emitPrepFiles.bundle.js && typia generate --input shared/checkers-raw --output shared/checkers",
    "pretest:type": "npm run checkers",
    "test:tsc": "tsc --esModuleInterop --noEmit --allowImportingTsExtensions ./shared/types/test/*.type.spec.ts"
  },
  "author": "",
  "license": "SEE LICENSE IN ./LICENSE",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/register": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "babel-loader": "^8.2.2",
    "node-watch": "^0.7.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.8.8",
    "tape": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsx": "^4.7.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.0.3",
    "typia": "^4.1.14",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-notifier": "^1.15.0"
  },
  "dependencies": {
    "@sjcrh/augen": "2.43.3-0",
    "@sjcrh/proteinpaint-rust": "2.44.0",
    "better-sqlite3": "^9.4.1",
    "body-parser": "^1.15.2",
    "canvas": "~2.11.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.5",
    "d3": "^7.6.1",
    "deep-object-diff": "^1.1.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.1.5",
    "got": "^11.5.1",
    "image-size": "^0.5.5",
    "jsonwebtoken": "^9.0.0",
    "jstat": "^1.9.3",
    "ky": "^1.2.1",
    "lazy": "^1.0.11",
    "micromatch": "^4.0.5",
    "minimatch": "^3.1.2",
    "node-fetch": "^2.6.1",
    "partjson": "^0.58.2",
    "tiny-async-pool": "^1.2.0",
    "typedoc-plugin-missing-exports": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stjude/proteinpaint",
    "directory": "server"
  },
  "files": [
    "bin.js",
    "dataset/*.mjs",
    "genome/*.mjs",
    "*server.js*",
    "start.js",
    "src/serverconfig.js",
    "src/lines2R.js",
    "src/mds3.gdc.filter.js",
    "src/checkReadingFrame.js",
    "src/bedj.parseBed.js",
    "routes/*.mjs",
    "utils/*.R",
    "LICENSE/*"
  ],
  "bugs": {
    "url": "https://github.com/stjude/pp-dist"
  },
  "browserify": {
    "transform": [
      "browserify-css",
      [
        "babelify",
        {
          "babelrc": false,
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "@babel/plugin-syntax-dynamic-import",
            "@babel/plugin-transform-runtime"
          ]
        }
      ]
    ]
  }
}
