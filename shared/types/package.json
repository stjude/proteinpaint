{
  "name": "@sjcrh/proteinpaint-types",
  "version": "2.86.0",
  "type": "module",
  "description": "Shared type definitions between ProteinPaint server and client code",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "exports": {
    ".": "./src/index.d.ts",
    "./updated": "./src/terms/updated-types.ts",
    "./*": "./src/*",
    "./routes/*": "./src/routes/*",
    "./checkers/*": "./dist/checkers/*"
  },
  "imports": {
    "#shared": "@sjcrh/proteinpaint-shared"
  },
  "scripts": {
    "prepare": "ts-patch install",
    "test": "tsc",
    "build": "./build.sh",
    "prepack": "./build.sh && mv src src-raw && mv srcjs src && mv package.json package.json-0 && mv src/package.json .",
    "postpack": "rm -rf src && mv src-raw src && mv package.json-0 package.json",
    "doc": "./doc.sh"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "typedoc": "^0.26.11",
    "typedoc-plugin-missing-exports": "^3.0.0",
    "typedoc-plugin-replace-text": "^4.0.0",
    "typescript": "^5.6.3",
    "typia": "^4.1.14"
  },
  "files": [
    "dist",
    "src/*.d.ts",
    "src/**/*.d.ts"
  ]
}
