{
  "name": "@sjcrh/proteinpaint-types",
  "version": "2.84.0",
  "type": "module",
  "description": "Shared type definitions between ProteinPaint server and client code",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./updated": "./src/terms/updated-types.ts",
    "./*": "./src/*",
    "./routes/*": "./src/routes/*",
    "./checkers/*": "./dist/checkers/*"
  },
  "imports": {
    "#shared": "@sjcrh/proteinpaint-shared"
  },
  "scripts": {
    "prepare": "ts-patch install",
    "test": "tsc",
    "build": "typia generate --input src/checkers --output dist",
    "doc": "./doc.sh"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "typia": "^4.1.14"
  },
  "peerDependencies": {
    "typescript": "^5.6.3"
  },
  "files": [
    "dist",
    "src"
  ]
}
