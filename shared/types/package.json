{
  "name": "@sjcrh/proteinpaint-types",
  "version": "2.86.0",
  "type": "module",
  "description": "Shared type definitions between ProteinPaint server and client code",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./updated": "./src/terms/updated-types.ts",
    "./routes/*": "./src/routes/*",
    "./checkers": "./dist/index.js"
  },
  "imports": {
    "#shared": "@sjcrh/proteinpaint-shared"
  },
  "scripts": {
    "predev": "tsx emitCheckers.ts",
    "dev": "node --watch-path=src --watch-path=checkers dev.mjs",
    "prepare": "ts-patch install",
    "build": "./build.sh",
    "prepack": "./build.sh",
    "postpack": "cd dist && ls *.js | grep -xvF index.js | xargs rm -f",
    "test": "tsc",
    "doc": "./doc.sh",
    "generate": "typia generate --input ./checkers --output ./dist"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "typedoc": "^0.26.11",
    "typedoc-plugin-missing-exports": "^3.0.0",
    "typedoc-plugin-replace-text": "^4.0.0",
    "typescript": "^5.6.3",
    "typia": "^4.1.14"
  },
  "files": [
    "dist/*.js",
    "src"
  ]
}
