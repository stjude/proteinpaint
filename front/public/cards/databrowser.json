{
  "intro": "By uploading a dictionary in the Data Browser, one can explore the structure of their custom data.",
  "ppcalls": [
    {
      "isUi": true,
      "runargs": {
        "noheader": true,
        "nobox": 1,
        "parseurl": false,
        "tkui": "databrowser"
      }
    },
    {
      "label": "Custom Data",
      "download": "https://proteinpaint.stjude.org/ppdemo/databrowser/dictionaryDemoData.tar.gz",
      "runargs": {
        "noheader": true,
        "nobox": true,
        "mass": {
          "state": {
            "vocab": {
              "terms": [
                {
                  "id": "Demographic",
                  "isleaf": false,
                  "name": "Demographic",
                  "parent_id": null
                },
                {
                  "id": "Prognosis",
                  "isleaf": false,
                  "name": "Prognosis",
                  "parent_id": null
                },
                {
                  "groupsetting": {
                    "disabled": true
                  },
                  "id": "Gender",
                  "isleaf": true,
                  "name": "Gender",
                  "parent_id": "Demographic",
                  "type": "categorical",
                  "values": {
                    "Female": {
                      "color": "pink",
                      "key": "Female",
                      "label": "Female"
                    },
                    "Male": {
                      "color": "lightblue",
                      "key": "Male",
                      "label": "Male"
                    }
                  }
                },
                {
                  "bins": {
                    "default": {
                      "bin_size": 0.5,
                      "first_bin": {
                        "stop": 0.5
                      },
                      "rounding": ".1f",
                      "startinclusive": true,
                      "type": "regular-bin"
                    }
                  },
                  "id": "Age",
                  "isleaf": true,
                  "parent_id": "Demographic",
                  "name": "Age at Diagnosis",
                  "type": "float",
                  "values": {}
                },
                {
                  "type": "categorical",
                  "values": {
                    "3-6 yrs": {
                      "key": "3-6 yrs",
                      "label": "3-6 yrs",
                      "color": "green"
                    },
                    "<3 yrs": {
                      "key": "<3 yrs",
                      "label": "<3 yrs",
                      "color": "orange"
                    }
                  },
                  "isleaf": true,
                  "id": "Age Group",
                  "name": "Age Group",
                  "parent_id": "Demographic",
                  "groupsetting": {
                    "disabled": true
                  }
                },
                {
                  "type": "categorical",
                  "values": {
                    "Dead ": {
                      "key": "Dead ",
                      "label": "Dead "
                    },
                    "Alive": {
                      "key": "Alive",
                      "label": "Alive"
                    }
                  },
                  "isleaf": true,
                  "id": "Alive/Dead",
                  "name": "Alive/Dead",
                  "parent_id": "Prognosis",
                  "groupsetting": {
                    "disabled": true
                  }
                },
                {
                  "groupsetting": {
                    "disabled": true
                  },
                  "id": "Progresssive Disease",
                  "isleaf": true,
                  "name": "Progresssive Disease",
                  "parent_id": "Prognosis",
                  "type": "categorical",
                  "values": {
                    "No": {
                      "color": "red",
                      "key": "No",
                      "label": "No"
                    },
                    "Yes": {
                      "color": "green",
                      "key": "Yes",
                      "label": "Yes"
                    }
                  }
                }
              ],
              "sampleannotation": {
                "0": {
                  "Age": 5,
                  "Gender": "Male",
                  "Alive/Dead": "Dead",
                  "Age Group": "3-6 yrs",
                  "Progresssive Disease": "No"
                },
                "1": {
                  "Age": 2,
                  "Gender": "Female",
                  "Alive/Dead": "Alive",
                  "Age Group": "<3 yrs",
                  "Progresssive Disease": "Yes"
                },
                "2": {
                  "Age": 4,
                  "Gender": "Male",
                  "Alive/Dead": "Alive",
                  "Age Group": "3-6 yrs",
                  "Progresssive Disease": "No"
                },
                "3": {
                  "Age": 6,
                  "Gender": "Male",
                  "Alive/Dead": "Alive",
                  "Age Group": "3-6 yrs",
                  "Progresssive Disease": "Yes"
                },
                "4": {
                  "Age": 5,
                  "Gender": "Male",
                  "Alive/Dead": "Alive",
                  "Age Group": "3-6 yrs",
                  "Progresssive Disease": "No"
                },
                "5": {
                  "Age": 4,
                  "Gender": "Male",
                  "Alive/Dead": "Alive",
                  "Age Group": "3-6 yrs",
                  "Progresssive Disease": "No"
                },
                "6": {
                  "Age": 2,
                  "Gender": "Male",
                  "Alive/Dead": "Alive",
                  "Age Group": "<3 yrs",
                  "Progresssive Disease": "No"
                },
                "7": {
                  "Age": 4,
                  "Gender": "Male",
                  "Alive/Dead": "Dead",
                  "Age Group": "3-6 yrs",
                  "Progresssive Disease": "Yes"
                },
                "8": {
                  "Age": 5,
                  "Gender": "Female",
                  "Alive/Dead": "Alive",
                  "Age Group": "3-6 yrs",
                  "Progresssive Disease": "Yes"
                },
                "9": {
                  "Age": 5,
                  "Gender": "Male",
                  "Alive/Dead": "Alive",
                  "Age Group": "3-6 yrs",
                  "Progresssive Disease": "No"
                }
              }
            },
            "plots": [
              {
                "chartType": "dictionary"
              }
            ]
          }
        }
      },
      "testSpec": {
        "timeout": 7000,
        "expected": {
          "button": 1
        }
      }
    },
    {
      "label": "Survivorship Study",
      "runargs": {
        "noheader": true,
        "nobox": true,
        "termdb": {
          "state": {
            "vocab": {
              "dslabel": "SJLife",
              "genome": "hg38"
            }
          }
        }
      },
      "buttons": [
        {
          "name": "Survivorship Study Homepage",
          "link": "https://survivorship.stjude.cloud/"
        }
      ],
      "testSpec": {
        "timeout": 7000,
        "expected": {
          "input": 1,
          "button": 1
        }
      }
    },
    {
      "label": "MSigDB",
      "runargs": {
        "noheader": true,
        "nobox": true,
        "termdb": {
          "state": {
            "vocab": {
              "dslabel": "msigdb",
              "genome": "hg38"
            }
          }
        }
      },
      "buttons": [
        {
          "name": "MSigDB Homepage",
          "link": "http://www.gsea-msigdb.org/gsea/msigdb/index.jsp"
        }
      ],
      "testSpec": {
        "timeout": 7000,
        "expected": {
          "input": 1,
          "button": 1
        }
      }
    }
  ],
  "buttons": [
    {
      "name": "Documentation",
      "link": "https://github.com/stjude/proteinpaint/wiki/Data-Browser"
    }
  ]
}